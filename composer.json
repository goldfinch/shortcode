{
    "name": "goldfinch/shortcode",
    "description": "A quick way to create custom shortcodes in Silverstripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "shortcode",
        "silverstripe-shortcode"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Art Karasev",
            "email": "art.karasev@proton.me"
        }
    ],
    "require": {
        "php": ">=8.0",
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0",
        "goldfinch/helpers": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Goldfinch\\Shortcode\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
